<%= render "header" %>
<h2><%= link_to "New Tweet", new_tweet_path %></h2>
<% @tweets.each do |tweet| %>
  <p><%= tweet.title %></p>
  <p><%= tweet.content %></p>
  <%= link_to "Show", tweet_path(tweet)%>
  <%= link_to "Edit", edit_tweet_path(tweet) %>
  <%= link_to 'Destroy', tweet_path(tweet),
                      method: :delete, data: { confirm: 'Are you sure?' } %>
<% end %>
<%= paginate @tweets %>
