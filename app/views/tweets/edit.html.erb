<h1>Edit</h1>
<%= form_for :tweet, url: tweet_path, method: :patch do |f| %>

  <p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :content %><br>
    <%= f.text_field :content %>
  </p>

  <p>
    <%= f.submit  %>
  </p>
<% end %>

<%= link_to "Back", tweets_path %>
